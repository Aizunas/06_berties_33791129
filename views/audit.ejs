<!DOCTYPE html>
<html>
<head>
    <title>Audit Log <%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
</head>
<body>
    <h1><%= shopData.shopName %></h1>
    <h2>Login Audit Log</h2>
    <table border="1">
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Action</th>
            <th>Timestamp</th>
            <th>Success</th>
        </tr>
        <% auditLog.forEach(function(entry) { %>
            <tr>
                <td><%= entry.id %></td>
                <td><%= entry.username %></td>
                <td><%= entry.action %></td>
                <td><%= entry.timestamp %></td>
                <td><%= entry.success ? 'Yes' : 'No' %></td>
            </tr>
        <% }); %>
    </table>
    <a href="/">Home</a>
</body>
</html>